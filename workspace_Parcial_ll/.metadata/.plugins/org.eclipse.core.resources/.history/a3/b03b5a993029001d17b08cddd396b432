#ifndef POKEMON_H_
#define POKEMON_H_

//=========== DECLARACION ENTIDAD ========== //
typedef struct{

	int numero;
	char nombre[100];
	char tipo[100];
	char tamanio[50];
	char ataqueCargado[100];
	int valorAtaque;
	int esVarioColor;
}ePokemon;

eParticipante* Participante_new();
eParticipante* Participante_newParametros(char* id, char* nombre, char* edad, char* idDificultad, char* score);
int Participante_delete(eParticipante* this);




#endif /* POKEMON_H_ */
